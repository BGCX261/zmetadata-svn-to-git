    <MD_DataIdentification>
      <citation>
        <!-- information on how the described data is cited -->
        <CI_Citation id="experiment.cite1">
          <title>
            <gco:CharacterString>generic
            Title</gco:CharacterString>
          </title>
          <alternateTitle>
            <gco:CharacterString>more specific Title / instead of
            "Characterstring" "ISSN" etc. can be used includes
            short notice of processing done for C3-processed
