<BODY onload="parseURLOptions()">
<div id="container">
	<input type="button" class="tabButton sizer" value="Farm" onclick="showLayer('lFarm')">
	<input type="button" class="tabButton sizer"  value="Coordinate" onclick="showLayer('lCoords')">
	<input type="button" class="tabButton sizer" value="Buffered area" onclick="showLayer('lRadius')">
	<input type="button" class="tabButton sizer" value="Map Name" onclick="showLayer('lMapName')"><br>
	
	<div id="lMapName" class="tab">
		<h3>Map Name Search</h3>
		<TABLE>
		<TR>
			<TD><INPUT TYPE="radio" NAME="rMapName" id="r50" checked></TD>
			<TD class="example">1:50 000 Topographic map</TD>
			<TD><INPUT TYPE="radio" NAME="rMapName" id="r250"></TD>
			<TD class="example">1:250 000 Topographic map</TD>
		</TR>
		<TR>
			<TD class="example">Example    2528AB</TD>
			<TD><INPUT TYPE="text" id="tbx50"></TD>
			<TD class="example">Example    2528</TD>
			<TD><INPUT TYPE="text" id="tbx250"></TD>
		</TR>
		</TABLE>
	</div>

	<div id="lFarm" class="tabOn">
		<h3>Farm Search</h3>
		<TABLE>
		<tr>
			<td>Province</td>
			<td colspan="2">
				<select id="cbxProvinces"></select>
			</td>
		</tr>
		<TR>			
			<TD class="example">Farm Name<b style="color:red;">*</b></TD>
			<TD><INPUT TYPE="text" id="tbxFarmName"></TD>
		</TR>		
		<TR>			
			<TD class="example">Farm Number</TD>
			<TD><INPUT TYPE="text" id="tbxFarmNumber"></TD>
		</TR>		
		
		</TABLE>
		<!--<input type="button" class="plainButton" value="Search" onclick="doFarmSearch()">-->
	</div>

	<div id="lCoords" class="tab">
		<h3>Coordinate Search</h3>
		<TABLE>
			<tr>
				<td class="example">Minimum Longitude</td>
				<td class="example">Maximum Longitude</td>
			</tr>
			<tr>
				<td><INPUT TYPE="text" id="tbxMinX" value="14"></td>
				<td><INPUT TYPE="text" id="tbxMaxX" value="33"></td>
			</tr>		
			
			<TR>
				<TD class="example">Minimum Latitude</TD>
				<td class="example">Maximum Latitude</td>
			</tr>
			<tr>
				<td><INPUT TYPE="text" id="tbxMinY" value="-35"></td>
				<td><INPUT TYPE="text" id="tbxMaxY" value="-22"></td>
			</tr>
		</TABLE>
		<!--<input type="button" class="plainButton" value="Search" onclick="doLatLongSearch()">-->
	</div>

	<div id="lRadius" class="tab">
		<h3>Buffered Area Search</h3>
		<TABLE>
		<TR>
			<td colspan="4">Find a deposit within <INPUT TYPE="text" id="tbxRadiusRadius" value="7000"> meters around the point with coordinates </td>	
		</TR>
		<TR>
			<TD class="example">Longitude</TD>
			<TD><INPUT TYPE="text" id="tbxRadiusLong" value="29.291"></TD>
			<TD class="example">Latitude</TD>
			<TD><INPUT TYPE="text" id="tbxRadiusLat" value="-24.518"></TD>
		</TR>		
		</TABLE>
		<!--<input type="button" class="plainButton" value="Search" onclick="doRadiusSearch()">-->
	</div>
</div>
<div id="lRefine" >
	<div class="example">Unique Reference number or range of Unique Reference numbers</div>
	<br>
<!--<small>A six-digit number allocated to unique identify a deposit</small>-->

<table cellsapce=2 border=1>
<tr><td colspan=2><div class="example">Example &nbsp;&nbsp;&nbsp;&nbsp;470890</div></td></tr>
<tr><td><input type = "text" size = 10 name = "uqref1" id="tbxUniqueRef" value = "" ></td></tr>
<tr><td colspan=3><div class="example">Example &nbsp;&nbsp;&nbsp;&nbsp;470890 to 470893</div></td></tr>
<tr>
<td>First No.<input type = "text" size = 10 value = "" id="tbxUniqueRefStart" ></td>
<td>Last No.<input type = "text" size = 10 value = "" id="tbxUniqueRefEnd"></td>
</tr>
</table>


